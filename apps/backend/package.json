{
  "name": "@spider-ai/backend",
  "version": "1.0.0",
  "description": "Spider AI Backend API Server",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx src/database/seed.ts",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "prisma studio",
    "clean": "rimraf dist coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@prisma/client": "^5.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.9.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.1",
    "socket.io": "^4.7.1",
    "redis": "^4.6.7",
    "nodemailer": "^6.9.3",
    "handlebars": "^4.7.7",
    "joi": "^17.9.2",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "archiver": "^5.3.1",
    "tmp": "^0.2.1",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.8",
    "@types/joi": "^17.2.3",
    "@types/uuid": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/archiver": "^5.3.2",
    "@types/tmp": "^0.2.3",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "tsc-alias": "^1.8.7",
    "prisma": "^5.0.0",
    "rimraf": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "api",
    "backend",
    "express",
    "typescript",
    "prisma",
    "claude-api"
  ]
}
